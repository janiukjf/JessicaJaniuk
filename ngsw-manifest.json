{
  "static.ignore": [
    "/assets/fonts/roboto/",
    "/assets/social/"
  ],
  "dynamic": {
    "group": [
      {
        "name": "api",
        "urls": {
          "https://jessica-janiuk-api.herokuapp.com/": {
            "match": "prefix"
          },
          "https://api.flickr.com/services/rest/": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "freshness",
          "maxAgeMs": 3600000,
          "maxEntries": 40,
          "strategy": "lru"
        }
      }
    ]
  },
  "external": {
   "urls": [
      {
        "url": "https://fonts.googleapis.com/css?family=Material+Icons"
      },
      {
        "url": "https://use.typekit.net/dlk7ypt.js"
      }
    ]
  },
  "routing": {
    "index": "/",
    "routes": {
      "/": {
        "prefix": false
      },
      "/about": {
        "prefix": true
      },
      "/blog": {
        "prefix": true,
        "onlyWithoutExtension": true
      },
      "/r2d2": {
        "prefix": true,
        "onlyWithoutExtension": true
      },
      "/connect": {
        "prefix": true,
        "onlyWithoutExtension": true
      },
      "/photography": {
        "prefix": true
      }
    }
  }
}
