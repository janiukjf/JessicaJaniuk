{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/scss/**",
      "**/src/**",
      "**/e2e/**"
    ]
  }
}
